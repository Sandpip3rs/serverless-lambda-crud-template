{
  "name": "serverless-lambda-template",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "type": "commonjs",
  "scripts": {
    "start": "serverless offline --httpPort=4000",
    "deploy:production": "serverless deploy --aws-profile prd-profile --stage prd",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --coverage",
    "lint": "eslint '*/**/*.{js,ts}' --fix",
    "prettier": "prettier --write '*/**/*.{js,ts}'",
    "docs": "redoc-cli serve docs/openapi/openapi.yml",
    "docs:watch": "redoc-cli serve docs/openapi/openapi.yml --watch"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "aws-sdk": "^2.1176.0",
    "cross-fetch": "^3.1.5",
    "http-errors": "^2.0.0",
    "middy-middleware-json-error-handler": "^3.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@serverless/typescript": "^3.19.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "commitlint": "^11.0.0",
    "esbuild": "^0.14.49",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^7.2.12",
    "husky": "8.0.1",
    "jest": "^28.1.2",
    "json-schema-to-ts": "^2.5.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.7.1",
    "redoc-cli": "^0.13.16",
    "serverless": "^3.20.0",
    "serverless-certificate-creator": "^1.6.0",
    "serverless-domain-manager": "^6.0.3",
    "serverless-esbuild": "^1.31.0",
    "serverless-ignore": "^0.2.1",
    "serverless-offline": "^8.8.1",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-plugin-canary-deployments": "^0.8.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.7.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "Sandpipers",
  "license": "MIT"
}
